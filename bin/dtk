#!/usr/bin/env ruby
module DTK
  require_relative '../lib/client'
  module GlobalForDSL
    Error  = ::DTK::Client::Error
    OsUtil = ::DTK::Client::OsUtil
  end
  #dtk_dsl must be loaded adter GlobalForDSL set
  require 'dtk_dsl'
  # lib/cli must be loaded after lib/client and lib/dsl
  require_relative '../lib/cli'

  Client::CLI::Runner.run(ARGV)
end
